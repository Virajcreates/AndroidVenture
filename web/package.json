{
  "name": "androidventure-web-viewer",
  "version": "1.0.0",
  "description": "Web viewer for displaying processed frames from Android edge detection app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "npx http-server . -p 8080",
    "server": "node server.js",
    "dev": "concurrently \"npm:serve\" \"npm:server\""
  },
  "keywords": [
    "typescript",
    "opencv",
    "frame-viewer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "ws": "^8.18.0"
  }
}
